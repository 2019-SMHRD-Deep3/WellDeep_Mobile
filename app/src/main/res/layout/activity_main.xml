<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/et_default_drawable"
    tools:context=".MainActivity">

    <Button
        android:id="@+id/btn_question"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_marginTop="40dp"
        android:background="@drawable/question"
        android:textColor="#53435b"
        app:layout_constraintEnd_toEndOf="@+id/btn_video"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/btn_video"
        app:layout_constraintTop_toBottomOf="@+id/indicator" />

    <Button
        android:id="@+id/btn_childlist"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:background="@drawable/all"
        android:textColor="@color/design_default_color_on_secondary"
        app:layout_constraintBottom_toBottomOf="@+id/btn_video"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/btn_child"
        app:layout_constraintTop_toTopOf="@+id/btn_video" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="204dp"
        android:layout_marginEnd="1dp"
        android:text="님 환영합니다"
        android:textColor="#1E1D1D"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="@+id/btn_modify"
        app:layout_constraintEnd_toStartOf="@+id/imageView3"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView3"
        app:layout_constraintVertical_bias="0.631" />

    <TextView
        android:id="@+id/tv_id"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="152dp"
        android:text="TextView"
        android:textColor="#191819"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="@+id/textView2"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/textView2"
        app:layout_constraintVertical_bias="0.0" />

    <Button
        android:id="@+id/btn_modify"
        android:layout_width="93dp"
        android:layout_height="31dp"
        android:layout_marginStart="128dp"
        android:layout_marginTop="90dp"
        android:background="@drawable/et_draw"
        android:text="회원정보수정"
        android:textColor="#E3A305"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toEndOf="@+id/textView2"
        app:layout_constraintTop_toTopOf="@+id/imageView3" />

    <Button
        android:id="@+id/btn_child"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:background="@drawable/register"
        android:textColor="#1A1A1B"
        app:layout_constraintBottom_toBottomOf="@+id/btn_video"
        app:layout_constraintEnd_toStartOf="@+id/btn_childlist"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/btn_view"
        app:layout_constraintTop_toTopOf="@+id/btn_video"
        app:layout_constraintVertical_bias="0.0" />

    <Button
        android:id="@+id/btn_video"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:background="@drawable/video"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/btn_view"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/viewPager"
        app:layout_constraintVertical_bias="0.763" />

    <Button
        android:id="@+id/btn_view"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:background="@drawable/alram"
        android:textColor="#121112"
        app:layout_constraintBottom_toBottomOf="@+id/btn_video"
        app:layout_constraintEnd_toStartOf="@+id/btn_child"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/btn_video"
        app:layout_constraintTop_toTopOf="@+id/btn_video"
        app:layout_constraintVertical_bias="1.0" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="영상보기"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/textView3"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btn_video"
        app:layout_constraintVertical_bias="0.26999998" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="알림보기"
        app:layout_constraintBottom_toBottomOf="@+id/textView"
        app:layout_constraintEnd_toStartOf="@+id/textView4"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/textView"
        app:layout_constraintTop_toTopOf="@+id/textView" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="아이등록"
        app:layout_constraintBottom_toBottomOf="@+id/textView"
        app:layout_constraintEnd_toStartOf="@+id/textView5"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/textView3"
        app:layout_constraintTop_toTopOf="@+id/textView" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="자녀보기"
        app:layout_constraintBottom_toBottomOf="@+id/textView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/textView4"
        app:layout_constraintTop_toTopOf="@+id/textView" />

    <me.relex.circleindicator.CircleIndicator
        android:id="@+id/indicator"
        android:layout_width="413dp"
        android:layout_height="30dp"
        app:ci_drawable="@drawable/black_radius"
        app:ci_height="7dp"
        app:ci_margin="4dp"
        app:ci_width="7dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/viewPager" />

    <androidx.viewpager.widget.ViewPager
        android:id="@+id/viewPager"
        android:layout_width="410dp"
        android:layout_height="250dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btn_modify">

    </androidx.viewpager.widget.ViewPager>

    <Button
        android:id="@+id/btn_map"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:background="@drawable/map"
        app:layout_constraintBottom_toBottomOf="@+id/btn_question"
        app:layout_constraintEnd_toEndOf="@+id/btn_view"
        app:layout_constraintStart_toStartOf="@+id/btn_view"
        app:layout_constraintTop_toTopOf="@+id/btn_question" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:text="문의하기"
        app:layout_constraintEnd_toEndOf="@+id/textView"
        app:layout_constraintStart_toStartOf="@+id/textView"
        app:layout_constraintTop_toBottomOf="@+id/btn_question" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="찾아오기"
        app:layout_constraintBottom_toBottomOf="@+id/textView10"
        app:layout_constraintEnd_toEndOf="@+id/btn_map"
        app:layout_constraintStart_toStartOf="@+id/btn_map"
        app:layout_constraintTop_toTopOf="@+id/textView10" />

    <Button
        android:id="@+id/btn_web"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:background="@drawable/web"
        app:layout_constraintBottom_toBottomOf="@+id/btn_question"
        app:layout_constraintEnd_toEndOf="@+id/btn_child"
        app:layout_constraintStart_toStartOf="@+id/btn_child"
        app:layout_constraintTop_toTopOf="@+id/btn_question" />

    <Button
        android:id="@+id/btn_center"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_marginEnd="40dp"
        android:background="@drawable/center"
        app:layout_constraintBottom_toBottomOf="@+id/btn_web"
        app:layout_constraintEnd_toEndOf="@+id/btn_childlist"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/btn_childlist"
        app:layout_constraintTop_toTopOf="@+id/btn_web" />

    <TextView
        android:id="@+id/textView12"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="웹페이지"
        app:layout_constraintBottom_toBottomOf="@+id/textView10"
        app:layout_constraintEnd_toEndOf="@+id/btn_web"
        app:layout_constraintStart_toStartOf="@+id/btn_web"
        app:layout_constraintTop_toTopOf="@+id/textView10" />

    <TextView
        android:id="@+id/textView13"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="복지센터"
        app:layout_constraintBottom_toBottomOf="@+id/textView10"
        app:layout_constraintEnd_toEndOf="@+id/btn_center"
        app:layout_constraintStart_toStartOf="@+id/btn_center"
        app:layout_constraintTop_toTopOf="@+id/textView10" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="wrap_content"
        android:layout_height="80dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/finnal" />

</androidx.constraintlayout.widget.ConstraintLayout>